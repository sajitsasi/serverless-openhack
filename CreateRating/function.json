{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "links",
      "methods": [
        "post"
      ]
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "byoc",
      "databaseName": "byoc-db",
      "collectionName": "ratings",
      "createIfNotExists": "true",
      "connectionStringSetting": "AzureCosmosDBConnectionString"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
